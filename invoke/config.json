{
    "hide_output": true,
    "url": "https://download.rockylinux.org/pub/rocky/8/isos/x86_64/Rocky-8.5-x86_64-minimal.iso",
    "files-dir": "../files/",
    "template": {
        "name": "rockylinux",
        "variables": {
            "hostname": "homelab",
            "timezone": ""
        },
        "output-ext": ".ks"
    },
    "iso-owner": "libvirt-qemu",
    "libvirt-images-dir": "/var/lib/libvirt/images/",
    "vm-properties": {
        "disk-size": 25,
        "format": "qcow2",
        "ram": 2048,
        "vcpus": 2,
        "os-variant": "generic",
        "network": "default"
    }
}